		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="IdBody" type="xs:int">
				<xs:annotation>
					<xs:documentation>Уникальный номер Body</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BodyGl1" type="xs:string">
				<xs:annotation>
					<xs:documentation>Текст наполнения 1</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="BodyGl2" type="xs:string">
