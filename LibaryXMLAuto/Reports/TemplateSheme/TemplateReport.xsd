	<xs:element name="Template">
		<xs:annotation>
			<xs:documentation>Описание документа поля и заголовки</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Headers"/>
				<xs:element ref="Body"/>
				<xs:element ref="Stone"/>
			</xs:sequence>
			<xs:attribute name="IdTemplate" type="xs:int">
				<xs:annotation>
					<xs:documentation>Ун документа</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IdHeaders" type="xs:int">
				<xs:annotation>
					<xs:documentation>Ун заголовка</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IdBody" type="xs:int">
				<xs:annotation>
					<xs:documentation>Ун наполнения</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="IdStone" type="xs:int">
				<xs:annotation>
					<xs:documentation>Ун окончания</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DateCreateTemplate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата создания</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Document">
		<xs:annotation>
			<xs:documentation>Имя документа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="NameDocument"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TemplateFull" type="Document">
