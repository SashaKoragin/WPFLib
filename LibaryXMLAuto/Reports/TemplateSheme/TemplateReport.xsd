			<xs:attribute name="TextHeade7" type="xs:string">
				<xs:annotation>
					<xs:documentation>Текст шапки 7</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="TextHeade8" type="xs:string">
				<xs:annotation>
					<xs:documentation>Текст шапки 8</xs:documentation>
