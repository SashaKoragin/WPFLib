<UserControl
    x:Class="TestDynamicElement.MoveXaml.View.MoveXaml1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:datacontext="clr-namespace:TestDynamicElement.MoveXaml.ViewModel"
    xmlns:local="clr-namespace:TestDynamicElement.MoveXaml.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DataContext="{d:DesignInstance {x:Type datacontext:ViewModel}, IsDesignTimeCreatable=True}"
    d:DesignHeight="300"
    
    d:DesignWidth="300"
    
    mc:Ignorable="d">
    <!--  Искуственно усложним отражения Шаблона что бы показать Шаблон может быть разный главное реализация  -->
    <StackPanel>
        <ItemsControl ItemsSource="{Binding Model.User2, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <StackPanel>

                        <ContentControl Content="{Binding Control}" />

                    </StackPanel>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </StackPanel>
</UserControl>
