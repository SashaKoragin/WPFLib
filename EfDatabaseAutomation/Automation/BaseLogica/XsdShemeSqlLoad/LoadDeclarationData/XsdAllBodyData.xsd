				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InnBank" type="xs:string" use="required" sql:datatype="varchar">
				<xs:annotation>
					<xs:documentation>ИНН Банка</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="KppBank" type="xs:string" use="required" sql:datatype="varchar">
