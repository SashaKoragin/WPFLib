					<xs:documentation>Логика кнопки</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="InnUl" type="xs:string">
				<xs:annotation>
					<xs:documentation>ИНН</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FullStatus" type="xs:string">
