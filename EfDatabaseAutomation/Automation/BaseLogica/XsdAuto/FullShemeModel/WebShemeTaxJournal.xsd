	</xs:element>
	<xs:element name="IsLk3" type="xs:boolean" nillable="true">
		<xs:annotation>
			<xs:documentation>ЛК3</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IsPrint" type="xs:boolean" nillable="true">
