		<xs:annotation>
			<xs:documentation>121 статья</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Id"/>
				<xs:element ref="IsLk3"/>
				<xs:element ref="IsMail"/>
				<xs:element ref="IsPrint"/>
				<xs:element ref="IsTks"/>
