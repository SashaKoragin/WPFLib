			<xs:documentation>Лицо для добавления (Предпроверка)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Id"/>
			</xs:sequence>
			<xs:attribute name="LogicsButton" type="xs:string" default="Button">
