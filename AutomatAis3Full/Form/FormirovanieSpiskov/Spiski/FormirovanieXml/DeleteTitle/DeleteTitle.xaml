<UserControl x:Class="AutomatAis3Full.Form.FormirovanieSpiskov.Spiski.FormirovanieXml.DeleteTitle.DeleteTitle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:datacontext="clr-namespace:AutomatAis3Full.Form.FormirovanieSpiskov.Spiski.DataContext"
             xmlns:local="clr-namespace:AutomatAis3Full.Form.FormirovanieSpiskov.Spiski.FormirovanieXml.DeleteTitle"
             d:DataContext="{d:DesignInstance {x:Type datacontext:DataContextSpisok}}"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <WrapPanel>
        <CheckBox
            Margin="5,5,5,5"
            Content="Убрать заголовок!!!"
            IsChecked="{Binding CheckBoxModel.IsCheced}"
            ToolTip="Участвует ли 1 строка в списке или нет!!!" />
    </WrapPanel>
</UserControl>
